html(lang="en")
  head
    title= 'Ghumio web console'
    link(rel='stylesheet', href='/stylesheets/webcommander.css')
    script(type='text/javascript', src='/javascripts/jquery-1.10.2.js')
    script(type='text/javascript', src='/javascript/jquery-2.1.1.min.js')
    script(type='text/javascript').
      var socket = io.connect('http://localhost:3000');
      socket.on('fromMachine', function (data) {
        console.log(data);
        socket.emit('fromBrowser', 'WebCommander connected');
      });
  body
    include #{Panels}
